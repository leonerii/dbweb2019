extends layout

block content
  .w3-container
    .w3-card-4.w3-teal
      h1 Classificação e Avaliação da Informação Pública
    table.w3-table-all
        tr
          th Sigla
          th Designação
          th Sioe
          th Estado
          th Internacional
        each doc in lista
          tr
            td=doc.sigla
            td=doc.designacao
            td=doc.sioe
            td=doc.estado
            td=doc.internacional
    address
      footer.w3-container.w3-card-4.w3-teal
        address 
          a(href="http://127.0.0.1:3001") Homepage
        